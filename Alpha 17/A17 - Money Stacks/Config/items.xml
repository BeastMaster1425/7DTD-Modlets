<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<!-- NB: stack size cannot exceed 32768 [signed int max] without the stack disappearing when logging back in -->
	<set xpath="/items/item[@name='casinoCoin']/property[@name='Stacknumber']/@value">32000</set>
	<append xpath="/items">
		<item name="compressedCoin">
			<property name="Extends" value="casinoCoin"/>
			<property name="CustomIcon" value="casinoCoin"/>
			<property name="CustomIconTint" value="5555ff"/>
			<property name="SellableToTrader" value="false"/>
			<property name="Stacknumber" value="50"/>
		</item>
	</append>
</configs>