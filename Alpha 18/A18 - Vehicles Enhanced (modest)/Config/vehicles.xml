<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<append xpath="/vehicles/vehicle[@name='vehicleMotorcycle']">
		<property class="seat1">
			<property name="position" value="0, .15, -.5"/>
			<property name="rotation" value="0, 0, 0"/>
			<property name="exit" value="-1.3,0,.9 ~ 1.3,0,.9 ~ 0,0,-2.4 ~ 0,0,2.4 ~ 0,2,0"/>
		</property>
	</append>
	
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@class='engine']/property[@name='fuelKmPerL']/@value">0.15</set> <!-- default: 0.1 -->
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax']/@value">12, 18</set> <!-- default: "9, 13" -->
	
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@class='engine']/property[@name='fuelKmPerL']/@value">0.25</set> <!-- default: 0.2 -->
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax']/@value">13, 21</set> <!-- default: "9, 15" -->
</configs>
