<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<append xpath="/vehicles/vehicle[@name='vehicleMotorcycle']">
		<property class="seat1">
			<property name="position" value="0, .15, -.5"/>
			<property name="rotation" value="0, 0, 0"/>
			<property name="exit" value="-1.3,0,.9 ~ 1.3,0,.9 ~ 0,0,-2.4 ~ 0,0,2.4 ~ 0,2,0"/>
		</property>
	</append>
	<append xpath="/vehicles/vehicle[@name='vehicleGyrocopter']">
		<property class="seat1">
			<property name="position" value=".06, -.2, -1.5"/>
			<property name="rotation" value="-25, 270, 0"/>
			<property name="exit" value="-1.3,0,.9 ~ 1.3,0,.9 ~ 0,0,-2.4 ~ 0,0,2.4 ~ 0,2,0"/>
		</property>
		<property class="seat2">
			<property name="position" value="-.06, -.2, -1.5"/>
			<property name="rotation" value="-25, 90, 0"/>
			<property name="exit" value="-1.3,0,.9 ~ 1.3,0,.9 ~ 0,0,-2.4 ~ 0,0,2.4 ~ 0,2,0"/>
		</property>
	</append>
	
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@class='engine']/property[@name='fuelKmPerL']/@value">0.2</set> <!-- default: 0.1 -->
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax']/@value">15, 22</set> <!-- default: "9, 14" -->
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='motorTorqueMinQ']/@value">7500</set> <!-- default: 5000 -->
	<set xpath="/vehicles/vehicle[@name='vehicle4x4Truck']/property[@name='motorTorqueMaxQ']/@value">7500</set> <!-- default: 5000 -->
	
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@class='engine']/property[@name='fuelKmPerL']/@value">0.3</set> <!-- default: 0.2 -->
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax']/@value">16, 24</set> <!-- default: "9, 14" -->
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='motorTorqueMinQ']/@value">7500</set> <!-- default: 3000 -->
	<set xpath="/vehicles/vehicle[@name='vehicleMotorcycle']/property[@name='motorTorqueMaxQ']/@value">7500</set> <!-- default: 3000 -->
	
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@class='engine']/property[@name='fuelKmPerL']/@value">0.3</set> <!-- default: 0.15 -->
	<set xpath="/vehicles/vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax']/@value">15, 24</set> <!-- default: "9, 15" -->
</configs>
